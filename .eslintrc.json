{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console" : "off",
        "quotes": [
            "error",
            "single"
        ],
        "semi" : [2, "always"],
        "import/prefer-default-export" : "off",
        "padded-blocks" : "off",
        "no-param-reassign":"off",
        "max-len" :"off"
    }
}
